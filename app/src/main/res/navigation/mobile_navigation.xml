    <fragment
        android:id="@+id/nav_input_expense"
        android:name="com.example.carwash.ui.inputexpense.ExpenseDataFragment"
        android:label="@string/menu_input_expense"
        tools:layout="@layout/fragment_expense_data" >
        <action
            android:id="@+id/action_nav_input_expense_to_nav_expense_checkout"
            app:destination="@id/nav_expense_checkout" />
    </fragment>

    <fragment
        android:id="@+id/nav_expense_checkout"
        android:name="com.example.carwash.ui.inputexpense.ExpenseCheckoutFragment"
