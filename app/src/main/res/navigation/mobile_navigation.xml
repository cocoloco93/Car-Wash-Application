        <action
            android:id="@+id/action_nav_input_data_to_nav_check_out"
            app:destination="@id/nav_check_out" />
    </fragment>

    <fragment
        android:id="@+id/nav_check_out"
        android:name="com.example.carwash.ui.inputdata.CheckoutFragment"
        android:label="@string/checkout"
        tools:layout="@layout/fragment_check_out" />


    <!--Expense Records-->
    <fragment
        android:id="@+id/nav_expense_records"
        android:name="com.example.carwash.ui.expenserecords.ExpenseRecordsFragment"
