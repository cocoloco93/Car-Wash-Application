    <!--Income Records-->
    <fragment
        android:id="@+id/nav_income_records"
        android:name="com.example.carwash.ui.IncomeRecords.IncomeRecordsFragment"
        android:label="@string/menu_income_records"
        tools:layout="@layout/fragment_income_records" />


    <!--Car Number Records-->
    <fragment
        android:id="@+id/nav_car_number_records"
        android:name="com.example.carwash.ui.carnumberecords.CarNumberRecordsFragment"
