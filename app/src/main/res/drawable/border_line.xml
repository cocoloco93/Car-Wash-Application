<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="#FFFFFF" />
            <stroke
                android:dashGap="3dp"
                android:dashWidth="1dp"
                android:width="2dp"
                android:color="#A4A4A4" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <corners android:radius="7dp"/>
        </shape>
    </item>
</layer-list>