<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ShopInfo.ManagerCreationFragment">

    <!-- TODO: Update blank fragment layout -->


    <EditText
        android:id="@+id/edtextPassword3"
        android:layout_width="335dp"
        android:layout_height="41dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:drawableLeft="@drawable/password"
        android:drawablePadding="5dp"
        android:ems="10"
        android:maxLength="20"
        android:hint="**********"
        android:inputType="textPassword"
        android:background="@drawable/border_line"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textPassword3" />

    <TextView
        android:id="@+id/textPassword3"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:text="@string/comfirm_password"
        android:textColor="@color/bulecolor"
        android:textSize="20sp"
        app:fontFamily="@font/pyidaungsu_"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtextPassword2" />

    <Button
        android:id="@+id/btmCreate4"
        android:layout_width="251dp"
        android:layout_height="43dp"
        android:layout_marginStart="80dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/rounded_buttom"
        android:ellipsize="none"
        android:text="@string/create"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:fontFamily="@font/segoeui"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/edtextPassword2"
        android:layout_width="335dp"
        android:layout_height="41dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:drawableLeft="@drawable/password"
        android:drawablePadding="5dp"
        android:ems="10"
        android:hint="**********"
        android:background="@drawable/border_line"
        android:maxLength="20"
        android:inputType="textPassword"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textPassword2" />

    <TextView
        android:id="@+id/textPassword2"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:text="@string/password"
        android:textColor="@color/bulecolor"
        android:textSize="20sp"
        app:fontFamily="@font/pyidaungsu_"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtextUName" />

    <EditText
        android:id="@+id/edtextUName"
        android:layout_width="335dp"
        android:layout_height="41dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:drawableLeft="@drawable/name"
        android:drawablePadding="5dp"
        android:ems="10"
        android:focusable="true"
        android:gravity="center"
        android:hint="mrbanana"
        android:background="@drawable/border_line"
        android:inputType="textEmailSubject"
        android:maxLength="30"
        android:textAlignment="textStart"
        android:textSize="15sp"
        app:fontFamily="@font/pyidaungsu_"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textUname"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textUname"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="75dp"
        android:fontFamily="@font/pyidaungsu_"
        android:gravity="start"
        android:text="@string/user_name"
        android:textAlignment="textStart"
        android:textColor="@color/bulecolor"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>