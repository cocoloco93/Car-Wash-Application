<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUpActivity">

    <EditText
        android:id="@+id/edSlogan2"
        android:layout_width="280dp"
        android:layout_height="49dp"
        android:background="@null"
        android:gravity="center"
        android:text="@string/slogan"
        android:textColor="@color/bulecolor"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.227" />

    <EditText
        android:id="@+id/edtextComName"
        android:layout_width="280dp"
        android:layout_height="33dp"
        android:background="@null"
        android:gravity="center"
        android:text="@string/car_wash_company"
        android:textColor="@color/bulecolor"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textWelcome"
        app:layout_constraintTop_toBottomOf="@+id/textSignUp"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/textSignIn"
        android:layout_width="75dp"
        android:layout_height="30dp"
        android:layout_gravity="start"
        android:gravity="center"
        android:text="@string/sign_in"
        android:textAlignment="textStart"
        android:textColor="@color/colorAccent"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.761"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.881"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textAlreadyAcc2"
        android:layout_width="160dp"
        android:layout_height="30dp"
        android:layout_marginTop="28dp"
        android:gravity="center"
        android:text="@string/already_have_an_account"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.873"
        tools:ignore="RtlCompat" />

    <EditText
        android:id="@+id/edtextComPassword"
        android:layout_width="351dp"
        android:layout_height="41dp"
        android:drawableLeft="@drawable/password"
        android:gravity="center"
        android:hint="**********"
        android:drawablePadding="5dp"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.644"
        tools:ignore="RtlCompat" />

    <EditText
        android:id="@+id/edtextPassword"
        android:layout_width="351dp"
        android:layout_height="41dp"
        android:drawableLeft="@drawable/password"
        android:gravity="center"
        android:hint="**********"
        android:textAlignment="textStart"
        android:drawablePadding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.535"
        tools:ignore="RtlCompat" />

    <EditText
        android:id="@+id/edtextGmail"
        android:layout_width="351dp"
        android:layout_height="41dp"
        android:drawablePadding="5dp"
        android:drawableLeft="@drawable/username"
        android:hint="abc@gmail.com"
        android:textAlignment="textStart"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.425"
        android:gravity="center"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/password"
        android:textColor="@color/bulecolor"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.082"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <TextView
        android:id="@+id/textEmail"
        android:layout_width="51dp"
        android:layout_height="22dp"
        android:text="@string/email"
        android:textColor="@color/bulecolor"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.371" />

    <TextView
        android:id="@+id/textComPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/comfirm_password"
        android:textColor="@color/bulecolor"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.586" />

    <TextView
        android:id="@+id/textSignUp"
        android:layout_width="164dp"
        android:layout_height="55dp"
        android:gravity="center"
        android:text="@string/sign_up"
        android:textAlignment="textStart"
        android:textColor="#A3000000"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textWelcome"
        android:layout_width="86dp"
        android:layout_height="23dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:gravity="center"
        android:text="@string/welcome_to"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textSignUp"
        app:layout_constraintVertical_bias="0.058"
        tools:ignore="RtlCompat" />

    <Button
        android:id="@+id/btmEditShop"
        android:layout_width="251dp"
        android:layout_height="43dp"
        android:background="@drawable/rounded_buttom"
        android:ellipsize="none"
        android:text="@string/sign_up"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.793" />


</androidx.constraintlayout.widget.ConstraintLayout>