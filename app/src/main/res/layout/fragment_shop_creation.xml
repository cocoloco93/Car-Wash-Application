<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <EditText
        android:id="@+id/edtextTax"
        android:layout_width="140dp"
        android:layout_height="33dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/border_line"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="@string/tax"
        android:maxLength="10"
        android:textAlignment="textStart"
        android:textSize="15sp"
        app:fontFamily="@font/pyidaungsu_"
        app:layout_constraintStart_toEndOf="@+id/checkboxTax"
        app:layout_constraintTop_toBottomOf="@+id/edtextAddress"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textforSignIn10"
        android:layout_width="189dp"
        android:layout_height="30dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="75dp"
        android:gravity="center_horizontal"
        android:text="@string/shop_name"
        android:textAlignment="textStart"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp"
        android:textStyle="bold"
        app:fontFamily="@font/segoeui"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/textforSignIn12"
        android:layout_width="189dp"
        android:layout_height="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:gravity="center_horizontal"
        android:text="@string/shop_address"
        app:fontFamily="@font/segoeui"
        android:textAlignment="textStart"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtextPhoneNumber"
        tools:ignore="RtlCompat" />

    <Spinner
        android:id="@+id/spinnerCity"
        android:layout_width="140dp"
        android:layout_height="33dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/droup_down"
        android:drawablePadding="5dp"
        android:tooltipText="@string/car_type"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textforSignIn12" />

    <EditText
        android:id="@+id/edtextShopName"
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/border_line"
        android:ems="10"
        android:fontFamily="@font/segoeubold"
        android:gravity="center_horizontal"
        android:hint="@string/enter_shop_name"
        android:maxLength="20"
        android:textAlignment="textStart"
        android:textSize="15sp"
        app:fontFamily="@font/pyidaungsu_"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textforSignIn10"
        tools:ignore="RtlCompat" />

    <EditText
        android:id="@+id/edtextPhoneNumber"
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:background="@drawable/border_line"
        android:gravity="center_horizontal"
        android:hint="@string/enter_phone_number"
        app:fontFamily="@font/pyidaungsu_"
        android:textAlignment="textStart"
        android:ems="10"
        android:maxLength="20"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textforSignIn11"
        tools:ignore="RtlCompat" />

    <EditText
        android:id="@+id/edtextAddress"
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/border_line"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="@string/enter_shop_address"
        android:maxLength="50"
        android:textAlignment="textStart"
        android:textSize="15sp"
        app:fontFamily="@font/pyidaungsu_"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerCity"
        tools:ignore="RtlCompat" />

    <Spinner
        android:id="@+id/spinnerTownShip"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/droup_down"
        app:layout_constraintStart_toEndOf="@+id/spinnerCity"
        app:layout_constraintTop_toBottomOf="@+id/textforSignIn12" />

    <TextView
        android:id="@+id/textforSignIn11"
        android:layout_width="189dp"
        android:layout_height="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:gravity="center_horizontal"
        android:text="@string/phone_number"
        app:fontFamily="@font/segoeui"
        android:textAlignment="textStart"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtextShopName"
        tools:ignore="RtlCompat" />

    <CheckBox
        android:id="@+id/checkboxTax"
        android:layout_width="140dp"
        android:layout_height="33dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="15dp"
        android:checked="true"
        android:gravity="center_horizontal"
        android:text="@string/tax"
        android:textAlignment="textStart"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp"
        android:textStyle="bold"
        app:fontFamily="@font/segoeui"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtextAddress"
        tools:ignore="RtlCompat" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginBottom="30dp"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="16dp">

        <Button
            android:id="@+id/btmCreate4"
            android:layout_width="251dp"
            android:layout_height="43dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/rounded_buttom"
            android:ellipsize="none"
            android:text="@string/set_up"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="bold"
            app:fontFamily="@font/segoeui"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>